{
    "name": "namtomo/workflow",
    "description": "A simple php workflow package",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^1.22"
    }
}
